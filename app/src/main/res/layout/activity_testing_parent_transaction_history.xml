<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TestingParentTransactionHistory"
    android:orientation="vertical">


    <Button
        android:id="@+id/PreOrderHistory"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="PreOrder History"/>


    <Button
        android:id="@+id/walkInOrderHistory"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Walk-In Order History"/>

    <Button
        android:id="@+id/QRPaymentHistory"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="QR Payment History"/>



</LinearLayout>